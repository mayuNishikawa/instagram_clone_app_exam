<h1>Log in</h1>

<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <div class="form_innerWrap">
    <div class="field">
      <%= f.label :email %>
      <%= f.email_field :email %>
    </div>

    <div class="field">
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>

    <div class="button">
      <%= f.submit "Log in" %>
    </div>
  </div>
<% end %>